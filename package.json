{
  "name": "@stormid/scaffold",
  "version": "0.2.0",
  "description": "Project scaffold and UI build system",
  "scripts": {
    "build": "webpack --config tools/webpack/config/build",
    "ci": "webpack --config tools/webpack/config/integration",
    "dev": "webpack serve --config tools/webpack/config/dev --hot",
    "lint": "node_modules/.bin/eslint src -c .eslintrc --ext js",
    "start": "npm run dev",
    "storybook": "start-storybook -p 6006",
    "storybook:eject": "build-storybook -c .storybook -o storybook",
    "test": "jest",
    "watch": "npm run watch-dev",
    "watch-dev": "webpack --config tools/webpack/config/integration --watch",
    "build-storybook": "build-storybook"
  },
  "author": "stormid",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^@templates(.*)$": "<rootDir>/src/templates$1",
      "^@layouts(.*)$": "<rootDir>/src/templates/layouts$1",
      "^@components(.*)$": "<rootDir>/src/templates/components$1"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@stormid/eslint-config": "^1.0.2",
    "@storybook/addon-a11y": "6.3.2",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-storysource": "6.3.2",
    "@storybook/preact": "^6.5.10",
    "autoprefixer": "^9.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.3.0",
    "eval": "^0.1.4",
    "fast-async": "^7.0.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.2.10",
    "postcss-css-variables": "^0.18.0",
    "postcss-custom-properties": "^13.3.0",
    "postcss-loader": "^7.3.3",
    "preact": "^10.16.0",
    "preact-render-to-json": "^3.6.6",
    "preact-render-to-string": "^6.2.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^2.8.15",
    "@stormid/toggle": "^1.0.0-alpha.18",
    "es6-object-assign": "^1.1.0",
    "picturefill": "^3.0.3",
    "promise-polyfill": "^8.1.0"
  }
}
